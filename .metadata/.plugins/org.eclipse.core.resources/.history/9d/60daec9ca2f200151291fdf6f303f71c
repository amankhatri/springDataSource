package collections;

class IsPrime{
	
	public static void checkPrime(int num){
		if(!(num >= 0 || num <=50)){
			System.out.println("error");
			return;
		}
		for (int i = 2; i < num/2; i++){
			if ( num%i == 0){
				System.out.println("no");
				return;
			}
		}
		System.out.println("yes");
	}
}

public class TestPrime {

	public static void main(String[] args) {
		

	}

}
